        <div class="dropdown-menu" id="dropdownList">
              <a class="dropdown-item  focus-red" href="/pt/estatistica/energia/petroleo-e-derivados/vendas-mensais/">Vendas mensais</a>
              <a class="dropdown-item  " href="/pt/estatistica/energia/petroleo-e-derivados/vendas-anuais/">Vendas anuais</a>
              <a class="dropdown-item  " href="/pt/estatistica/energia/petroleo-e-derivados/importacoes-exportacoes/">Importa&#231;&#245;es/Exporta&#231;&#245;es</a>
        </div>
      </div>
        <script src="/scripts/jquery-3.3.1.min.js"></script>
        <script src="/scripts/bootstrap.min.js"></script>
        <script>
            $(function () {
              $('.btn-group.open').on({
                  'show.bs.dropdown': function(e) {
                      var element;
                      var elementClicked = e.clickEvent;
                      if (elementClicked != undefined)
                          element = e.clickEvent.target;
                      else
                           element = e.target;
                             if (element.className == "btn-group"){
                              var elementPageKey = $('#0672f734-474f-487b-a976-60bb1851b56b');
                               elementPageKey.find($(".fa-chevron-right")).toggleClass('fa-chevron-right fa-chevron-down');
                               elementPageKey.find($(".fa-chevron-down")).toggleClass('.fa-chevron-down .fa-chevron-right');
                              return true;
                            }
                              if (element.className == "btn-group open"){
                                var elementPageKey = $('#0672f734-474f-487b-a976-60bb1851b56b');
                                 elementPageKey.find($(".fa-chevron-right")).toggleClass('fa-chevron-right fa-chevron-down');
                                return true;
                              }
                  },
                  'hide.bs.dropdown': function(e) {
                      var element;
                      var elementClicked = e.clickEvent;  
                      if (elementClicked != undefined)
                          element = e.clickEvent.target;
                      else
                          element = e.target;
                      if (element.className == "btn-group open show" ) {
                          return  true;
                      }
                       if (element.className == "btn-group show" && element.className != "btn-group open show"  ) {
                          return  true;
                      }
                      else {
                          e.preventDefault();
                      }
                  }
              });
                    $('#0672f734-474f-487b-a976-60bb1851b56b').dropdown('toggle');
          });
        </script>
        <script>
                    $(function () {
                      $('.btn-group').hover(function () {
                        var FirstChild = $(this).children('.btn-group');
                        if (FirstChild == '')
                          console.log("First Child Vazio");
                        FirstChild.children('button').addClass('hihover');
                      }, function () {
                        var FirstChild = $(this).children('.btn-group');
                        if (FirstChild == '')
                          console.log("First Child Vazio");
                        FirstChild.children('button').removeClass('hihover');
                      });
                    });
        </script>
    </div>
  </div>
</div>

